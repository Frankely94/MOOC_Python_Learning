year = int(input("Year:"))

while True: 
    
    if year % 400 == 0 and year % 100 == 0:
       
        t = True   
        break
    elif year % 4 == 0 and year % 100 != 0 and year % 400 == 0:
        t = True  
        break
    else:
        t = False       
        break

if t == True:
    print(f"The next leap yearrr after {year} is {year + 4}")
elif t == False:
    count = year + 1
    if (count % 400 == 0 and count % 100 == 0  ) or (count % 4 == 0 and count % 100 != 0):
        print(f"The next leap yearrr after {year} is {count}")
        
   

            
            
    
        

 
            
    
        




       
   

            
        
    
   


       
     
       
       
   
    
  



      
    
    
        

    
